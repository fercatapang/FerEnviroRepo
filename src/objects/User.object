    </fields>
    <fields>
        <fullName>LLC_BI__nCino_User__c</fullName>
        <deprecated>false</deprecated>
        <description>This field is automatically populated as enabled. This field indicates whether the user has a license for the nCino Platform or not.</description>
        <externalId>false</externalId>
        <formula>TRUE</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>nCino User</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ManagerId</fullName>
