    <fields>
        <fullName>nPortal__Has_Reached_Ownership_Limit__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This checkbox field indicates if the user has reached the configured maximum number of owned Accounts for role optimization</description>
        <externalId>false</externalId>
        <label>Has Reached Ownership Limit</label>
        <trackFeedHistory>false</trackFeedHistory>
